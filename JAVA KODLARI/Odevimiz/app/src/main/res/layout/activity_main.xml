<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"


    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bulut"
    android:hapticFeedbackEnabled="false"
    android:scrollbarSize="5dp"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imgRehber"
        android:layout_width="0dp"
        android:layout_height="68dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="27dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toStartOf="@+id/imgMail"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/telq" />

    <ImageView
        android:id="@+id/imgKitap"
        android:layout_width="72dp"
        android:layout_height="61dp"
        android:layout_marginEnd="160dp"
        android:layout_marginBottom="3dp"
        android:scrollbarSize="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/bbb" />

    <ImageView
        android:id="@+id/imgDedek"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="33dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgMail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgMail"
        app:layout_constraintTop_toTopOf="@+id/imgRehber"
        app:srcCompat="@drawable/cccc" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="275dp"
        android:text="AMBULANS"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imgAmbulans" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="102dp"
        android:layout_marginEnd="7dp"
        android:text="REHBER"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imgRehber"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:text="DEDEKTÖR"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="@+id/imgDedek"
        app:layout_constraintTop_toBottomOf="@+id/imgRehber" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="83dp"
        android:layout_height="0dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="1dp"
        android:text="GAZ ACİL"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/imgGaz" />

    <ImageView
        android:id="@+id/imgİtfaye"
        android:layout_width="74dp"
        android:layout_height="0dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgAmbulans"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgGaz"
        app:srcCompat="@drawable/iiii" />

    <ImageView
        android:id="@+id/imgAmbulans"
        android:layout_width="74dp"
        android:layout_height="0dp"
        android:layout_marginStart="69dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgGaz"
        app:srcCompat="@drawable/zzz" />

    <ImageView
        android:id="@+id/imgGaz"
        android:layout_width="75dp"
        android:layout_height="64dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/imgİtfaye"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:srcCompat="@drawable/gazzzzz" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="14dp"
        android:text="İTFAYE"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imgİtfaye"
        app:layout_constraintTop_toBottomOf="@+id/imgİtfaye" />

    <ImageView
        android:id="@+id/imgMail"
        android:layout_width="0dp"
        android:layout_height="58dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintEnd_toStartOf="@+id/imgDedek"
        app:layout_constraintStart_toEndOf="@+id/imgRehber"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ggg" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="19dp"
        android:text="ACİL DURUM GMAİL"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/imgDedek"
        app:layout_constraintTop_toBottomOf="@+id/imgMail" />


</androidx.constraintlayout.widget.ConstraintLayout>

